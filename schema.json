{
    "$schema":"https://specs.frictionlessdata.io/schemas/table-schema.json",
    "name":"hautes-remunerations",
    "title": "Spécification du fichier de déclaration des hautes rémunérations dans la fonction publique",
    "description": "Spécification du fichier de déclaration des dix rémunérations les plus élevées des agents publiques",
    "countrycode": "FR",
    "homepage": "http://github.com/restuccia/schema-hautes-remunerations",
    "licenses":[
       {
          "title":"Etalab Licence Ouverte 2.0",
          "name":"etalab-2.0",
          "path":"https://spdx.org/licenses/etalab-2.0.html"
       }
    ],
    "sources":[
       {
          "title":"LOI n° 2019-828 du 6 août 2019 de transformation de la fonction publique",
          "path":"https://www.legifrance.gouv.fr/jorf/id/JORFTEXT000038889182"
       }
    ],
    "created": "2020-09-23",
    "lastModified": "2020-09-23",
    "version": "0.1.0",
    "contributors":[
       {
          "title":"Mario Restuccia",
          "email":"mario.restuccia@data.gouv.fr",
          "organisation":"Etalab",
          "role":"author"
       }
     ],
    "fields": [
        {
            "name": "siren",
            "description": "Le numéro SIREN de l'organisme",
            "example": "110046018",
            "type": "string",
            "constraints": {
                "pattern": "^\\d{8}$",
                "required": true,
                "unique": true
            }
        },
        {
            "name": "denomination_employeur",
            "description": "Dénomination de l'employeur",
            "example": "Ministère de la Culture",
            "type": "string",
            "constraints": {
                "required": true
            }
        },
        {
            "name": "annee",
            "description": "Année",
            "example": "2019",
            "type": "year",
            "constraints": {
                "required": true
            }
        },
        {
            "name": "somme_10_plus_hautes_remunerations",
            "description": "Somme des 10 plus hautes rémunérations brutes en euros",
            "example": "1609769",
            "type": "number",
            "constraints": {
                "required": true
            }
        },
        {
            "name": "nb_femmes_beneficiaires",
            "description": "Nombre de femmes bénéficiaires",
            "example": "5",
            "type": "integer",
            "constraints": {
                "required": true,
                "minimum": 0,
                "maximum": 10
            }
        },
        {
            "name": "nb_hommes_beneficiaires",
            "description": "Nombre d'hommes bénéficiaires",
            "example": "5",
            "type": "integer",
            "constraints": {
                "required": true,
                "minimum": 0,
                "maximum": 10
            }
        },
        {
            "name": "duree_cumulee_mois",
            "description": "Durée cumulée en nombre de mois",
            "example": "120",
            "type": "number",
            "constraints": {
                "required": true
            }
        },
        {
            "name": "commentaires",
            "description": "Commentaires",
            "example": "Commentaire",
            "type": "string",
            "constraints": {
                "required": false
            }
        }
    ],
    "missingValues": [
        ""
    ]
}
